//Vucut kitle indeksi hesaplamasi


#include<stdio.h>
int main()
{
    char VA , U, C;
    float VK;
    printf("Cinsiyet: ");      
	scanf("%s", &C);
    printf("Vucut agirligi(kg): ");        
	scanf("%d", &VA);
    printf("Boy uzunlugu(m): ");
    scanf("%f", &U);
    VK=VA/(U*U);
    printf("Vucut kitle indeksi: %fKg/m^2\n", VK);
    if (VK<15)
    printf("zayif");
    else   if(VK>=15 && VK<30)
	printf("Normal");
	else
	printf("Obez");    

    return 0;
}